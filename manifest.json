{
  "name": "Hello World Dev Quiz",
  "short_name": "HW Dev Quiz",
  "description": "Quiz interactif Chrome pour développeurs : testez vos connaissances sur les syntaxes Hello World dans plus de 100 langages. Fun, rapide, éducatif, responsive et open-source.",
  "version": "1.4",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": {
      "16": "hello_extensions.png",
      "32": "hello_extensions.png",
      "48": "hello_extensions.png",
      "128": "hello_extensions.png"
    }
  },
  "icons": {
    "16": "hello_extensions.png",
    "32": "hello_extensions.png",
    "48": "hello_extensions.png",
    "128": "hello_extensions.png"
  },
  "author": "Abdelhakim Baalla",
  "homepage_url": "https://github.com/Abdelhakim-Baalla/Hello-Extension-Chrome",
  "keywords": [
    "hello world", "quiz", "développeur", "programmation", "langages", "chrome extension", "dev", "fun", "éducatif", "test", "syntaxe", "open-source", "challenge", "code", "learning", "javascript", "python", "java", "c", "typescript", "rust", "php", "ruby", "swift", "kotlin", "go", "c#", "scala", "bash", "linux", "windows", "macos", "cloud", "web", "apprentissage", "challenge dev", "culture dev"
  ],
  "categories": [
    "Productivité", "Développement", "Éducation", "Quiz", "Programmation", "Open Source", "Jeux", "Apprentissage"
  ],
  "permissions": [],
  "offline_enabled": true,
  "default_locale": "fr",
  "support_url": "https://github.com/Abdelhakim-Baalla/Hello-Extension-Chrome/issues",
  "minimum_chrome_version": "100",
  "background": {
    "service_worker": "popup.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["hello_extensions.png"],
      "matches": ["<all_urls>"]
    }
  ]
}